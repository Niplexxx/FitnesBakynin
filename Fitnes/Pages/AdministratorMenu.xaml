<Page x:Class="Fitnes.Pages.AdministratorMenu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Fitnes.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdministratorMenu">

    <Grid>
        <StackPanel VerticalAlignment="Top">
            <Label Content="Меню:" Grid.Row="1" HorizontalAlignment="Center"/>
        </StackPanel>
        <StackPanel VerticalAlignment="Center">
            <Button Content="Регистрация тренера" x:Name="ButtonCoachRegistration" Click="ButtonCoachRegistration_Click" Margin="5" Width="130" Height="18" FontFamily="Times New Roman" Background="White"/>
        </StackPanel>
    </Grid>
</Page>
